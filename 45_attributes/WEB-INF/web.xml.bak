</servlet-mapping>

<servlet>
	<servlet-name>app2</servlet-name>
	<servlet-class>BBBServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>app2</servlet-name>
	<url-pattern>/bbb.do</url-pattern>
</servlet-mapping>
   
</web-app>
